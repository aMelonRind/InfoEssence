{
  "schemaVersion": 1,
  "id": "infoessence",
  "version": "${version}",
  "name": "InfoEssence",
  "description": "Can read item stats (mostly mmo items) and display some calculated stats in the item tooltip.",
  "authors": [
    "MelonRind"
  ],
  "contact": {
    "repo": "https://github.com/aMelonRind/InfoEssence"
  },
  "license": "GPL-3.0",
  "icon": "assets/infoessence/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "io.github.amelonrind.infoessence.InfoEssence"
    ],
    "modmenu": [
      "io.github.amelonrind.infoessence.config.ModMenuApiImpl"
    ]
  },
  "mixins": [
    "infoessence.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  }
}
